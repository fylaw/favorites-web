{
   "background": {
      "scripts": [ "js/axios.min.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/logo.png",
      "default_title": "Pinbox"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Ctrl+Shift+S",
            "linux": "Ctrl+Shift+S",
            "mac": "Command+Shift+S",
            "windows": "Ctrl+Shift+S"
         }
      }
   },
   "content_scripts": [ {
      "css": [ "css/content.css" ],
      "js": [ "js/keyCodeMap.js", "js/content.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "您的专属网络收藏夹，将文章、视频或喜欢的任何内容一键保存到 Pinbox, 随时随地收藏互联网",
   "homepage_url": "https://microsoftedge.microsoft.com/addons/detail/lhikococflpkoiaigndpmmmkeohibeai",
   "icons": {
      "128": "img/logo_128.png",
      "16": "img/logo_16.png",
      "48": "img/logo.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pinbox - 跨平台收藏",
   "options_page": "options/index.html",
   "permissions": [ "contextMenus", "bookmarks", "tabs", "cookies", "storage", "http://*/*", "https://*/*" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "3.1.0",
   "web_accessible_resources": [ "img/logo.png", "img/logo.svg", "save.html", "js/build.js" ]
}
